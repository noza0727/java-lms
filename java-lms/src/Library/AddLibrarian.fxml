<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Library.AddLibrarianController">
    <children>
        <Label layoutX="180.0" layoutY="38.0" text="Add Librarian" textFill="#835858">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <VBox layoutX="98.0" layoutY="105.0" prefHeight="190.0" prefWidth="283.0" spacing="15.0">
            <children>
                <TextField fx:id="lib_ID_add" prefHeight="35.0" prefWidth="225.0" promptText="ID" />
                <TextField fx:id="lib_Fname_add" prefHeight="35.0" prefWidth="225.0" promptText="First Name" />
                <TextField fx:id="lib_Lname_add" prefHeight="35.0" prefWidth="225.0" promptText="Last Name" />
                <PasswordField fx:id="lib_pass_add" editable="false" prefHeight="35.0" prefWidth="225.0" promptText="Password" />
            </children>
        </VBox>
        <HBox alignment="CENTER" layoutX="64.0" layoutY="314.0" prefHeight="48.0" prefWidth="352.0" spacing="50.0">
            <children>
                <Button mnemonicParsing="false" onAction="#clearAdd" prefHeight="32.0" prefWidth="81.0" text="Clear" textFill="#835858">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="32.0" prefWidth="81.0" text="Cancel" textFill="#835858">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#addLib" prefHeight="32.0" prefWidth="81.0" text="Add" textFill="#835858">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </HBox>
      <Label fx:id="add_error" alignment="CENTER" layoutX="96.0" layoutY="295.0" prefHeight="17.0" prefWidth="283.0" textFill="#bc2a2a">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="closeButton" layoutX="505.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="28.0" prefWidth="30.0" text="X" textFill="#835858">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
    </children>
</AnchorPane>
