            <Font size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="124.0" fitWidth="134.0" layoutX="50.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@icons/Student_Reading-512.png" />
         </image>
      </ImageView>
      <Button fx:id="closeButtonS" layoutX="671.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="28.0" prefWidth="30.0" style="-fx-background-color: lightGray;" text="X" textFill="#835858">
