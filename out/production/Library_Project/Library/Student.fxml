                  <TableColumn fx:id="coltitle" prefWidth="106.00000762939453" text="Title" />
                  <TableColumn fx:id="colauthor" prefWidth="100.00001525878906" text="Author" />
                  <TableColumn fx:id="colyear" prefWidth="99.0" text="Publish Year" />
                  <TableColumn fx:id="colgenre" prefWidth="95.0" text="Genre" />
                  <TableColumn fx:id="colisbn" prefWidth="99.0" text="ISBN" />
