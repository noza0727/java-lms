<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: e5dfdf;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Library.StudentController">
   <children>
      <Label fx:id="name_show" layoutX="19.0" layoutY="131.0" prefHeight="40.0" prefWidth="423.0" text="Name" textFill="#835858">
         <font>
            <Font size="25.0" />
         </font></Label>
      <Label fx:id="id_show" layoutX="19.0" layoutY="171.0" prefHeight="33.0" prefWidth="203.0" text="ID" textFill="#835858">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox layoutX="50.0" layoutY="236.0" prefHeight="214.0" prefWidth="602.0" spacing="7.0">
         <children>
            <Label alignment="CENTER" prefHeight="39.0" prefWidth="602.0" text="My Books" textFill="#835858">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TableView fx:id="myBookTable" editable="true" layoutY="100.0" prefHeight="167.0" prefWidth="500.0">
